doctype html
head
    title Chess Home
    meta(charset='UTF-8')
    style
        include main.css

.topnav
    a(style="float: left" href='./run') « Back

div(style='text-align:center')
    h1(style='font-size: 80px') 🏃Runs
    h2(style='font-size: 80px') Recent Runs
    table
        tr
            th 🥇
            th 🥈
            th 🥉
            th 4th
            th date
        each row in rows
            tr
                td
                    a(href='./profile?id=' + row.first_id)=row.first_id
                td
                    a(href='./profile?id=' + row.second_id)=row.second_id
                td
                    a(href='./profile?id=' + row.third_id)=row.third_id
                td
                    a(href='./profile?id=' + row.forth_id)=row.forth_id
                td= row.game_date